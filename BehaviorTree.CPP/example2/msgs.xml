      </Fallback>
      <MyScript code="step7"/>
      <MyScript code="step8"/>
      <MyScript code="step9"/>
      <MyScript code="step10"/>
      <Fallback>
        <MyScript code="step11"/>
        <MyScript code="step12"/>
        <MyScript code="step13"/>
        <MyScript code="step14"/>
      </Fallback>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="MyScript"
            editable="true">
      <input_port name="code"/>
    </Action>
    <Action ID="MyScriptCondition"
            editable="true">
      <input_port name="code"/>
