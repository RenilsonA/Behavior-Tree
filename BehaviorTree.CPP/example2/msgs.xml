  <BehaviorTree ID="process">
    <Sequence>
      <MyScript code="step1"/>
      <MyScript code="step2"/>
      <MyScript code="step3"/>
      <Fallback>
        <MyScript code="step4"/>
        <Sequence>
          <MyScript code="step5"/>
          <MyScript code="step6"/>
